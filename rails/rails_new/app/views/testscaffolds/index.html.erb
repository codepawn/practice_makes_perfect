<p id="notice"><%= notice %></p>

<h1>Listing Testscaffolds</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testscaffolds.each do |testscaffold| %>
      <tr>
        <td><%= link_to 'Show', testscaffold %></td>
        <td><%= link_to 'Edit', edit_testscaffold_path(testscaffold) %></td>
        <td><%= link_to 'Destroy', testscaffold, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Testscaffold', new_testscaffold_path %>
