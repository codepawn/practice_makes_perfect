<div class="messages">
  <div class="container">

    <% @messages.each do |message| %>
    <div class="message">
      <p class="content"><%= message.content %></p>
      <p class="time"><%= message.created_at %></p>
    </div>
    <% end %>
    <%= link_to 'New Messageee', "messages/new" %>
    <a href="/posts">
      <div class="btn btn-default">
        bootstrap
      </div>
    </a>
  </div>
</div>
